{
  "version": 2,
  "name": "hetasinglar-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api-hetasinglar.onrender.com/api",
    "REACT_APP_WS_URL": "wss://api-hetasinglar.onrender.com",
    "REACT_APP_FRONTEND_URL": "https://hetasinglar.vercel.app",
    "GENERATE_SOURCEMAP": "false"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://api-hetasinglar.onrender.com/api",
      "REACT_APP_WS_URL": "wss://api-hetasinglar.onrender.com",
      "REACT_APP_FRONTEND_URL": "https://hetasinglar.vercel.app",
      "GENERATE_SOURCEMAP": "false"
    }
  }
}
