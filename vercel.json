{
  "version": 2,
  "name": "hetasinglar-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://apihetasinglar.duckdns.org/api",
    "REACT_APP_WS_URL": "wss://apihetasinglar.duckdns.org",
    "REACT_APP_FRONTEND_URL": "https://hetasinglar.vercel.app",
    "REACT_APP_ENVIRONMENT": "production",
    "PUBLIC_URL": "https://hetasinglar.vercel.app",
    "GENERATE_SOURCEMAP": "false",
    "REACT_APP_CACHE_ENABLED": "true",
    "REACT_APP_API_TIMEOUT": "30000",
    "REACT_APP_SECURE_COOKIES": "true"
  }
}
